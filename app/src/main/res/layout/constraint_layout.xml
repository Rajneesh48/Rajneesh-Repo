<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <!--For Spacing From Start (Box1)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_start_Box1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".02"/>
<!--For Spacing From Top (Box1)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_top_Box1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".01"/>
    <!--For Spacing From End (Box1)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_End_Box1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".98"/>
    <!--For Spacing From Bottom (Box1)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_Bottom_Box1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".16"/>

    <!--Box1-->
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/spacer_top_Box1"
        app:layout_constraintBottom_toTopOf="@+id/spacer_Bottom_Box1"
        android:background="@color/Blue"
        app:layout_constraintStart_toStartOf="@id/spacer_start_Box1"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box1"
    />
    <!--Box1 End Box 2 Start-->

<!--    Spacer between Box1 and Box2-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_top_box2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".17"/>

    <!--For Spacing From Bottom (Box2)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_Bottom_Box2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".3"/>
    <!--For Spacing From End (Box2)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_End_Box2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".65"/>
    <!--Box2-->
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Blue"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box2"
        app:layout_constraintStart_toStartOf="@id/spacer_start_Box1"
        app:layout_constraintTop_toTopOf="@id/spacer_top_box2"
        app:layout_constraintBottom_toBottomOf="@id/spacer_Bottom_Box2"
        />
    <!--Box2 End Box3 Start-->

    <!--    Spacer between Box2 and Box3-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_start_box3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".67"/>
    <!--Box3-->
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Blue"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box1"
        app:layout_constraintStart_toStartOf="@id/spacer_start_box3"
        app:layout_constraintTop_toTopOf="@id/spacer_top_box2"
        app:layout_constraintBottom_toBottomOf="@id/spacer_Bottom_Box2"
        />
    <!--Box3 End Box4 Start-->

    <!--For Spacing From Top (Box4,5,6)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_top_Box456"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".31"/>
    <!--For Spacing From Bottom (Box4,5,6)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_bottom_Box456"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".83"/>
<!--Box4-->
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Blue"
        app:layout_constraintStart_toStartOf="@id/spacer_start_box3"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box1"
        app:layout_constraintTop_toTopOf="@id/spacer_top_Box456"
        app:layout_constraintBottom_toBottomOf="@id/spacer_bottom_Box456"/>
    <!--Box4 End Box5 Start-->

    <!--For spacing End (Box6(1st vertical Rectangle))-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_End_Box56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".32"/>
    <!--For spacing start (Box5,6)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_Start_Box56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".34"/>

    <!--Box5,6-->
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Blue"
        app:layout_constraintStart_toStartOf="@id/spacer_Start_Box56"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box2"
        app:layout_constraintTop_toTopOf="@id/spacer_top_Box456"
        app:layout_constraintBottom_toBottomOf="@id/spacer_bottom_Box456"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Blue"
        app:layout_constraintStart_toStartOf="@id/spacer_start_Box1"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box56"
        app:layout_constraintTop_toTopOf="@id/spacer_top_Box456"
        app:layout_constraintBottom_toBottomOf="@id/spacer_bottom_Box456"/>
    <!--Box5,6 End Box 7,8,9 Start-->
    <!--For Spacing From Top (Box 7,8,9)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_Top_Box789"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".84"/>
    <!--For Spacing From Bottom (Box 7,8,9)-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/spacer_Bottom_789"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".98"/>
<!--    Box 7,8,9-->
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/spacer_start_Box1"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box56"
        app:layout_constraintTop_toTopOf="@id/spacer_Top_Box789"
        app:layout_constraintBottom_toBottomOf="@id/spacer_Bottom_789"
        android:background="@color/Blue"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/spacer_Start_Box56"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box2"
        app:layout_constraintTop_toTopOf="@id/spacer_Top_Box789"
        app:layout_constraintBottom_toBottomOf="@id/spacer_Bottom_789"
        android:background="@color/Blue"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/spacer_start_box3"
        app:layout_constraintEnd_toEndOf="@id/spacer_End_Box1"
        app:layout_constraintTop_toTopOf="@id/spacer_Top_Box789"
        app:layout_constraintBottom_toBottomOf="@id/spacer_Bottom_789"
        android:background="@color/Blue"
        />



</androidx.constraintlayout.widget.ConstraintLayout>